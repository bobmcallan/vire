   type=raw,value=latest,enable=${{ github.ref == 'refs/heads/main' }}\n\n      - name: Build and push\n        uses: docker/build-push-action@v6\n        with:\n          context: .\n          file: docker/Dockerfile\n          push: true\n          tags: ${{ steps.docker-meta.outputs.tags }}\n          labels: ${{ steps.docker-meta.outputs.labels }}\n          build-args: |\n            VERSION=${{ steps.meta.outputs.version }}\n            BUILD=${{ steps.meta.outputs.build }}\n            GIT_COMMIT=${{ steps.meta.outputs.commit }}\n          cache-from: type=gha,scope=vire-server\n          cache-to: type=gha,mode=max,scope=vire-server\n\n  deploy:\n    needs: push\n    if: github.ref == 'refs/heads/main'\n    runs-on: ubuntu-latest\n    concurrency:\n      group: fly-deploy-server\n      cancel-in-progress: false\n\n    steps:\n      - name: Setup flyctl\n        uses: superfly/flyctl-actions/setup-flyctl@master\n\n      - name: Deploy to Fly.io\n        env:\n          FLY_API_TOKEN: ${{ secrets.FLY_API_TOKEN }}\n        run: |\n          flyctl deploy \\\n            --app vire-pprod-server \\\n            --image ${{ env.REGISTRY }}/${{ github.repository_owner }}/vire-server:latest \\\n            --strategy rolling \\\n            --wait-timeout 300 \\\n            --yes\n"}],"isError":false,"timestamp":1772018683101}}
