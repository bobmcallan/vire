# Vire Environment Configuration
# Copy this file to .env and fill in your API keys

# EODHD API for stock prices and fundamentals
EODHD_API_KEY=your_eodhd_api_key_here

# Navexa API for portfolio data
NAVEXA_API_KEY=your_navexa_api_key_here

# Gemini API for AI analysis
GEMINI_API_KEY=your_gemini_api_key_here

# Environment (development, production)
VIRE_ENV=development

# Server configuration (optional)
VIRE_SERVER_PORT=8080

# Logging level (debug, info, warn, error)
VIRE_LOG_LEVEL=info
